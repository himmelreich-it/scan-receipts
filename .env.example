# Receipt Processor Configuration
# Copy this file to .env and update with your settings

# Anthropic API key (required)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Folder paths (optional - defaults provided)
INPUT_RECEIPTS_FOLDER=input
DONE_FOLDER=done
FAILED_FOLDER=failed

# Output files (optional - defaults provided)
XLSX_OUTPUT_FILE=expenses.xlsx
CSV_STAGING_FILE=receipts.csv