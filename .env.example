# Receipt Processor Configuration
# Copy this file to .env and update with your settings

# Anthropic API key (required)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Folder paths (optional - defaults provided)
INCOMING_RECEIPTS_FOLDER=../data/incoming
SCANNED_FOLDER=../data/scanned
IMPORTED_FOLDER=../data/imported
FAILED_FOLDER=../data/failed

# Output files (optional - defaults provided)
CSV_STAGING_FILE=../data/receipts.csv
XLSX_OUTPUT_FILE=../data/output.xlsx
