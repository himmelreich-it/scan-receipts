{
  "permissions": {
    "defaultMode": "acceptEdits",
    "allow": [
      "Bash(python:*)",
      "Bash(uv run:*:*)",
      "Bash(uv add:*:*)",
      "Bash(uv remove:*:*)",
      "Bash(ls:*)",
      "Bash(npx pyright:*)",
      "Bash(gh issue create:*)",
      "Bash(gh issue view:*)",
      "Bash(gh issue edit:*)",
      "Bash(gh issue develop:*)",
      "Bash(gh issue comment:*)",
      "Bash(gh pr create:*)",
      "Bash(gh pr view:*)",
      "Bash(gh pr comment:*)",
      "Bash(gh pr checkout:*)",
      "Bash(gh pr diff:*)",
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(git push:*)",
      "Bash(mkdir:*)",
      "Bash(mv:*)",
      "Bash(touch:*)",
      "Bash(tree:*)",
      "Bash(rm:*)",
      "Bash(rmdir:*)"
    ],
    "deny": [
      "Bash(curl:*)",
      "Read(./.env)",
      "Read(./.env.testing)",
      "Read(./.env.production)"
    ]
  }
}